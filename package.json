{
  "name": "babel-plugin-debug-macros",
  "version": "0.3.4",
  "description": "Debug macros and feature flag stripping",
  "keywords": [
    "babel",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chadhietala/babel-debug-macros"
  },
  "license": "MIT",
  "contributors": [
    "Chad Hietala <chadhietala@gmail.com>",
    "Kris Selden <kris.selden@gmail.com>",
    "Robert Jackson <me@rwjblue.com>"
  ],
  "main": "src/index.js",
  "files": [
    "src/",
    "!src/tests"
  ],
  "scripts": {
    "changelog": "lerna-changelog",
    "lint": "eslint .",
    "test": "npm-run-all lint test:jest",
    "test:jest": "jest"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/tests/**/*-test.js"
    ]
  },
  "dependencies": {
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@release-it-plugins/lerna-changelog": "^6.1.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^5.1.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "lerna-changelog": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "release-it": "^17.1.1"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
